include ../../mixins/provider

div.detail__gallery
  div.splide.gallery-slider(data-gallery-slider data-type='i')
    div.splide__track
      ul.splide__list
        each item in [0, 1, 2, 3]
          li.splide__slide.gallery-slider__slide
            div.gallery-slider__image
              img(src="@img/i-theme/detail.png", alt="image")